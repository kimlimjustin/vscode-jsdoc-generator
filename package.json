{
  	"name": "jsdoc-generator",
  	"publisher": "kimlimjustin",
  	"author": "Justin Maximillian Kimlim <kimlimjustin@gmail.com>",
  	"repository": {
  	  	"type": "git",
  	  	"url": "https://github.com/kimlimjustin/vscode-jsdoc-generator.git"
  	},
  	"license": "MIT",
  	"icon": "images/icon.png",
  	"displayName": "JSDoc Generator",
  	"description": "JSDoc template generator",
  	"version": "0.0.4",
  	"engines": {
  	  	"vscode": "^1.57.0"
  	},
  	"categories": [
  	  	"Other"
  	],
  	"activationEvents": [
  	  	"*"
  	],
  	"main": "./out/extension.js",
  	"contributes": {
  	  	"commands": [
  	  	  	{
  	  	  	  	"command": "jsdoc-generator.generateJSDoc",
  	  	  	  	"title": "Generate JSDoc"
  	  	  	}
  	  	],
  	  	"keybindings": [
  	  	  	{
  	  	  	  	"command": "jsdoc-generator.generateJSDoc",
  	  	  	  	"key": "alt+j",
  	  	  	  	"mac": "cmd+j"
  	  	  	}
  	  	]
  	},
  	"scripts": {
  	  	"vscode:prepublish": "yarn run compile",
  	  	"compile": "tsc -p ./",
  	  	"watch": "tsc -watch -p ./",
  	  	"pretest": "yarn run compile && yarn run lint",
  	  	"lint": "eslint src --ext ts",
  	  	"test": "node ./out/test/runTest.js"
  	},
  	"devDependencies": {
  	  	"@types/vscode": "1.57.0",
  	  	"@types/glob": "7.2.0",
  	  	"@types/mocha": "8.2.2",
  	  	"@types/node": "14.17.4",
  	  	"eslint": "7.29.0",
  	  	"@typescript-eslint/eslint-plugin": "4.28.0",
  	  	"@typescript-eslint/parser": "4.28.0",
  	  	"glob": "7.2.0",
  	  	"mocha": "8.4.0",
  	  	"typescript": "4.3.4",
  	  	"vscode-test": "1.5.2"
  	}
}
